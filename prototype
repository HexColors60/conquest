# prototype for Conquest
#
# $Id$
# 
i copyright 
i pkginfo 
i preinstall 
d none /opt ? ? ?
d none /opt/bin ? ? ?
d none /opt/conquest 0755 root conquest 
d none /opt/conquest/bin 0775 root conquest
d none /opt/conquest/lib 0775 root conquest
d none /opt/conquest/etc 0775 root conquest
f none /opt/conquest/bin/conquest=$SRCDIR/conquest 2771 root conquest 1 plock,tshar NULL
f none /opt/conquest/bin/conqoper=$SRCDIR/conqoper 2771 root conquest 1 plock,tshar NULL
f none /opt/conquest/bin/conqdriv=$SRCDIR/conqdriv 2771 root conquest 1 plock,tshar NULL
f none /opt/conquest/bin/conqstrat=$SRCDIR/conqstrat 0755 root conquest 
f none /opt/conquest/etc/TODO=$SRCDIR/TODO 0644 root conquest
f none /opt/conquest/etc/conqrule=$SRCDIR/conqrule 0644 root conquest
f none /opt/conquest/etc/README=$SRCDIR/README 0644 root conquest
f none /opt/conquest/etc/conqnews.doc=$SRCDIR/conqnews.doc 0644 root conquest
f none /opt/conquest/etc/conquest.doc=$SRCDIR/conquest.doc 0644 root conquest
v none /opt/conquest/etc/conquestrc=$SRCDIR/clean.conquestrc 0664 root conquest
v none /opt/conquest/etc/conquest_common.img=$SRCDIR/clean.conquest_common.img 0660 root conquest
v none /opt/conquest/etc/conquest.log=$SRCDIR/clean.conquest.log 0660 root conquest
f none /opt/conquest/lib/libconquest.so=$SRCDIR/libconquest.so 0644 root conquest
s none /opt/bin/conquest=/opt/conquest/bin/conquest 
s none /opt/bin/conqoper=/opt/conquest/bin/conqoper 
