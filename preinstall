# preinstall file for conquest... make sure the conquest group exists
#
# Jon Trulson 5/19/96
#
# $Id$
#
############################################################################

if /bin/grep "^conquest:" /etc/group >/dev/null 2>&1 
then
	:	# it's there
else
	# add it
	echo "Adding group: conquest"
	/usr/sbin/groupadd conquest
fi

